    "# Using readlines()\n",
    "file1 = open('values_to_sum.txt', 'r')\n",
    "Lines = file1.readlines()\n",
    "  \n",
    "count = 0\n",
    "\n",
    "sum_values = 0\n",
    "\n",
    "# Strips the newline character\n",
    "for line in Lines:\n",
    "    sum_values += float(line)\n",
    "\n",
    "print(sum_values)"
   ]
  }
